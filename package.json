{
  "name": "panthera-bug-busters-food-delivery",
  "version": "0.5.0",
  "private": true,
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.8",
    "reactstrap": "^9.1.4",
    "sharp": "^0.31.0",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "eqeqeq": [
        "error",
        "smart"
      ],
      "no-regex-spaces": "error",
      "no-useless-escape": "error",
      "spaced-comment": [
        "error",
        "always"
      ],
      "semi-style": [
        "error",
        "last"
      ],
      "semi-spacing": [
        "error",
        {
          "before": false,
          "after": false
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "no-trailing-spaces": "error",
      "no-tabs": "error",
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "indent": [
        "error",
        2
      ],
      "lines-around-comment": [
        "error",
        {
          "beforeBlockComment": true,
          "afterBlockComment": false,
          "beforeLineComment": true,
          "afterLineComment": false,
          "allowBlockStart": true,
          "allowBlockEnd": false,
          "allowObjectStart": true,
          "allowObjectEnd": false,
          "allowArrayStart": true,
          "allowArrayEnd": false
        }
      ],
      "line-comment-position": [
        "error",
        {
          "position": "above"
        }
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "brace-style": [
        "error",
        "1tbs"
      ],
      "block-spacing": [
        "error",
        "always"
      ],
      "arrow-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "array-bracket-spacing": [
        "error",
        "never"
      ],
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "always"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.js",
          "**/*.jsx"
        ],
        "rules": {
          "object-shorthand": "off",
          "multiline-ternary": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.0",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.3.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "react-test": "^0.17.4",
    "sass": "^1.54.9"
  }
}
